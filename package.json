    "@types/styled-components": "5.1.25",
    "@types/underscore": "1.11.4",
    "@umijs/fabric": "2.10.0",
    "@umijs/test": "3.5.24",
    "cross-env": "7.0.3",
    "dumi": "1.1.42",
    "father-build": "1.22.2",
    "gh-pages": "4.0.0",
